{
  "name": "@tuplo/series-with",
  "description": "Sequentially applies an asynchronous function to an array",
  "version": "0.0.0-development",
  "repository": "git@github.com:tuplo/series-with.git",
  "author": "Rui Costa",
  "license": "MIT",
  "keywords": [],
  "files": [
    "dist/**/*.js",
    "dist/index.d.ts"
  ],
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "devDependencies": {
    "@types/jest": "26.0.15",
    "@types/node": "14.14.6",
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "commitizen": "4.2.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.13.0",
    "eslint-config-airbnb-typescript": "12.0.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "microbundle": "0.12.4",
    "prettier": "2.1.2",
    "pretty-quick": "3.1.0",
    "rimraf": "3.0.2",
    "ts-jest": "26.4.3",
    "typescript": "4.0.5"
  },
  "scripts": {
    "build": "microbundle --target node --sourcemap false --format cjs,modern --tsconfig tsconfig.build.json",
    "ci:test": "jest --ci",
    "ci:test:coverage": "jest --ci --coverage",
    "lint": "eslint 'src/**'",
    "test": "jest --watch",
    "ts-check": "tsc --noEmit"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    }
  }
}
